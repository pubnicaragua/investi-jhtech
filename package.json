{
  "name": "investi-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx expo start --dev-client",
    "start:go": "npx expo start",
    "start:web": "npx expo start --web",
    "web": "npx expo start --web",
    "start:memory": "set NODE_OPTIONS=--max-old-space-size=8192 && npx expo start --dev-client --clear",
    "dev": "npx expo start --dev-client --clear",
    "prod": "npx expo start --clear --no-dev --minify",
    "start:fast": "npx expo start --dev-client --clear",
    "android": "npx expo run:android",
    "android:memory": "set NODE_OPTIONS=--max-old-space-size=8192 && npx expo run:android",
    "ios": "npx expo run:ios",
    "logs": "adb logcat -v time ReactNativeJS:V ReactNative:V Expo:V *:E",
    "logs:all": "adb logcat -v time",
    "logs:clear": "adb logcat -c",
    "build:dev": "eas build --profile development --platform android",
    "build:preview": "eas build --profile preview --platform android",
    "lint": "eslint .",
    "test": "jest",
    "reset": "rmdir /s /q node_modules && npm install",
    "clean": "rmdir /s /q .expo .metro node_modules\\.cache",
    "optimize": "optimize-bundle.bat",
    "analyze": "npx depcheck",
    "test:apis": "node scripts/test-market-apis.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.9",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.0.12",
    "@react-navigation/native-stack": "^7.3.9",
    "@react-navigation/stack": "^7.3.9",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.90.2",
    "clsx": "^2.1.1",
    "expo": "~53.0.0",
    "expo-asset": "~11.1.7",
    "expo-av": "~15.1.7",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-document-picker": "~13.1.6",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "i18next": "^24.2.0",
    "lucide-react-native": "^0.539.0",
    "nativewind": "^4.1.0",
    "patch-package": "^8.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.0.0",
    "react-is": "^18.2.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-tab-view": "^4.2.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.20.0",
    "tailwind-merge": "^3.3.1",
    "yahoo-finance2": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-react-jsx-source": "^7.27.1",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.28.2",
    "@playwright/test": "^1.42.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eas-cli": "^16.20.1",
    "eslint": "^9.0.0",
    "eslint-config-universe": "^12.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "postcss": "^8.4.35",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.1.2",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@sentry/.*|@react-native-community/.*)"
    ]
  }
}
